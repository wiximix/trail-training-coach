{
  "name": "coze-coding-dev-sdk",
  "version": "0.5.5",
  "description": "Coze Coding Dev SDK - 优雅的多功能 AI SDK，支持图片生成、视频生成、语音合成、语音识别、大语言模型和联网搜索",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "bin": {
    "coze-coding-ai": "./bin/coze-coding-ai.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "publish:beta": "bash scripts/publish-npm.sh",
    "publish:latest": "bash scripts/publish-npm.sh --tag latest",
    "publish:major": "bash scripts/publish-npm.sh --version-bump major --tag latest",
    "publish:minor": "bash scripts/publish-npm.sh --version-bump minor",
    "publish:dry": "bash scripts/publish-npm.sh --dry-run",
    "link:local": "npm link",
    "unlink:local": "npm unlink -g coze-coding-dev-sdk"
  },
  "keywords": [
    "coze",
    "coze-coding",
    "dev",
    "ai",
    "sdk",
    "image-generation",
    "video-generation",
    "text-to-speech",
    "speech-recognition",
    "llm",
    "search",
    "doubao",
    "seedream",
    "typescript"
  ],
  "author": "Coze Coding Dev Team <support@coze.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coze/coze-coding-dev-sdk.git",
    "directory": "packages/typescript"
  },
  "bugs": {
    "url": "https://github.com/coze/coze-coding-dev-sdk/issues"
  },
  "homepage": "https://github.com/coze/coze-coding-dev-sdk#readme",
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/lib-storage": "^3.958.0",
    "@langchain/core": "^1.1.8",
    "@langchain/openai": "^1.2.0",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "ora": "^9.0.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
