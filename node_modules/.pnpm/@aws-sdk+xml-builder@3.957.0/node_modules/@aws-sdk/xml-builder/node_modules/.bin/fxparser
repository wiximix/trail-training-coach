#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules/fast-xml-parser/src/cli/node_modules:/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules/fast-xml-parser/src/node_modules:/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules/fast-xml-parser/node_modules:/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules:/workspace/projects/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules/fast-xml-parser/src/cli/node_modules:/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules/fast-xml-parser/src/node_modules:/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules/fast-xml-parser/node_modules:/workspace/projects/node_modules/.pnpm/fast-xml-parser@5.2.5/node_modules:/workspace/projects/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../fast-xml-parser@5.2.5/node_modules/fast-xml-parser/src/cli/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../fast-xml-parser@5.2.5/node_modules/fast-xml-parser/src/cli/cli.js" "$@"
fi
