#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/projects/node_modules/.pnpm/coze-coding-dev-sdk@0.5.5_@types+pg@8.16.0_openai@6.15.0_zod@4.3.5_/node_modules/coze-coding-dev-sdk/bin/node_modules:/workspace/projects/node_modules/.pnpm/coze-coding-dev-sdk@0.5.5_@types+pg@8.16.0_openai@6.15.0_zod@4.3.5_/node_modules/coze-coding-dev-sdk/node_modules:/workspace/projects/node_modules/.pnpm/coze-coding-dev-sdk@0.5.5_@types+pg@8.16.0_openai@6.15.0_zod@4.3.5_/node_modules:/workspace/projects/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/projects/node_modules/.pnpm/coze-coding-dev-sdk@0.5.5_@types+pg@8.16.0_openai@6.15.0_zod@4.3.5_/node_modules/coze-coding-dev-sdk/bin/node_modules:/workspace/projects/node_modules/.pnpm/coze-coding-dev-sdk@0.5.5_@types+pg@8.16.0_openai@6.15.0_zod@4.3.5_/node_modules/coze-coding-dev-sdk/node_modules:/workspace/projects/node_modules/.pnpm/coze-coding-dev-sdk@0.5.5_@types+pg@8.16.0_openai@6.15.0_zod@4.3.5_/node_modules:/workspace/projects/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../coze-coding-dev-sdk/bin/coze-coding-ai.js" "$@"
else
  exec node  "$basedir/../coze-coding-dev-sdk/bin/coze-coding-ai.js" "$@"
fi
